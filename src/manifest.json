{
  "manifest_version": 2,
  "name": "GitHub Project Enhancer",
  "short_name": "GPE",
  "version": "0.4.0",
  "description": "Enhance GitHub Projects to add accordion UI and so on.",
  "icons": {
    "128": "assets/icon.png"
  },
  "author": "ryz310@gmail.com",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/projects/*"],
      "css": [
        "css/foldering.css",
        "css/storyPoint.css"
      ],
      "js": [
        "js/foldering.js",
        "js/storyPoint.js"
      ]
    }
  ]
}
